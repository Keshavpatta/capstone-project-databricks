    {
      "name": "5cedaab6",
      "displayName": "Untitled dataset",
      "queryLines": [
        "SELECT\r\n",
        "  txn_date,\r\n",
        "  total_transactions,\r\n",
        "  successful_transactions,\r\n",
        "  failed_transactions,\r\n",
        "  failure_rate,\r\n",
        "  online_transactions,\r\n",
        "  branch_transactions,\r\n",
        "  ROUND(online_transactions * 100.0 / total_transactions, 2) AS online_pct,\r\n",
        "  ROUND(branch_transactions * 100.0 / total_transactions, 2) AS branch_pct,\r\n",
        "  avg_amount,\r\n",
        "  median_amount\r\n",
        "FROM capstone_project.gold.transaction_kpi_mv\r\n",
        "ORDER BY txn_date DESC\r\n",
        "LIMIT 30"
      ]
    }
